{
    "name":"maven-client",
    "slug":"maven-client",
    "version":"0.0.2",
    "description":"A maven client to request packages and construct download urls.",
    "devDependencies":{
        "testbox":"^4.0"
    },
    "ignore":[
        "**/.*",
        "/test/",
        "/tests/"
    ],
    "installPaths":{
        "testbox":"testbox/"
    },
    "private":false,
    "location":"foundeo/maven-client#v0.0.2",
    "homepage":"https://github.com/foundeo/maven-client/",
    "documentation":"https://github.com/foundeo/maven-client/",
    "repository":{
        "type":"git",
        "URL":"https://github.com/foundeo/maven-client"
    },
    "bugs":"https://github.com/foundeo/maven-client/issues",
    "scripts":{
        "postVersion":"package set location='foundeo/maven-client#v`package version`'",
        "onRelease":"publish",
        "postPublish":"!git push && git push --tags"
    },
    "shortDescription":"Maven repository client written in CFML",
    "testbox":{
        "bundles":"",
        "directory":"tests.specs",
        "labels":"",
        "options":{},
        "recurse":true,
        "reporter":"",
        "runner":[
            {
                "default":""
            }
        ],
        "testBundles":"",
        "testSpecs":"",
        "testSuites":"",
        "verbose":true,
        "watchDelay":500,
        "watchPaths":"**.cfc"
    },
    "type":"modules"
}
